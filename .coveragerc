[report]
fail_under = 50
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if sys.version_info
    class .*\(.*(Error|Exception)\):
    ^ *\.\.\.$

[run]
branch = true
omit =
    tests/data/*
    tests/util/*
